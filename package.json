{
  "name": "react-native-userpic",
  "version": "2.0.1",
  "description": "Display user avatars in React Native like a pro with support for Gravatar, user initials, unique colors, badges, statuses, and more.",
  "author": "Nick Seryakov (https://github.com/kolking)",
  "license": "MIT",
  "homepage": "https://github.com/kolking/react-native-userpic#readme",
  "repository": {
    "type": "git",
    "url": "git+https://kolking@github.com/kolking/react-native-userpic.git"
  },
  "bugs": {
    "url": "https://github.com/kolking/react-native-userpic/issues"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "userpic",
    "avatar",
    "gravatar",
    "user picture",
    "user image",
    "user avatar",
    "initials",
    "badge"
  ],
  "main": "src/index",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "test": "jest --passWithNoTests",
    "coverage": "yarn test --coverage",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "validate": "yarn tsc && yarn lint --fix && yarn test -u",
    "validate-ci": "yarn tsc && yarn lint && yarn coverage",
    "install-all": "yarn && cd example && yarn && npx pod-install"
  },
  "dependencies": {
    "js-md5": "^0.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.2",
    "@types/js-md5": "^0.4.3",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
